{
    "name": "ms-auth-mirror-node",
    "version": "1.0.0",
    "description": "microservice auth mirror",
    "main": "app.js",
    "scripts": {
        "dev": "set NODE_ENV=dev && nodemon ./app.js",
        "dev:up": "docker-compose up --build",
        "dev:down": "docker-compose down -v --rmi all",
        "development": "export NODE_ENV=development && nodemon ./app.js",
        "staging": "export NODE_ENV=production && nodemon ./app.js",
        "production": "export NODE_ENV=production && nodemon ./app.js",
        "spec:dev": "jest --watch --coverage",
        "spec:backend": "jest --verbose",
        "spec:frontend": "jest --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lucasfrct/ms-auth-node.git"
    },
    "author": "Lucas Costa e Marcus Mariano",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/lucasfrct/ms-auth-node/issues"
    },
    "homepage": "https://github.com/lucasfrct/ms-auth-node#readme",
    "_moduleAliases": {
        "@root": ".",
        "@src": "./src",
        "@utils": "./src/utils",
        "@public": "./src/public",
        "@routes": "./src/routes",
        "@library": "./src/lib",
        "@routines": "./src/routines",
        "@middleware": "./src/middleware",
        "@authentication": "./src/authentication"
    },
    "dependencies": {
        "body-parser": "1.19.0",
        "cors": "2.8.5",
        "dotenv": "10.0.0",
        "express": "4.17.1",
        "fs": "0.0.1-security",
        "hybrid-crypto-js": "0.2.4",
        "module-alias": "2.2.2",
        "nodemon": "2.0.15",
        "pino": "7.8.1",
        "pino-pretty": "7.5.3",
        "uglify-js": "3.15.1"
    },
    "devDependencies": {
        "jest": "27.5.1",
        "module-alias-jest": "0.0.3"
    }
}
